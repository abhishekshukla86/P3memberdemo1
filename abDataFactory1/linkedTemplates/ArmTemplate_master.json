{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "abDataFactory1"
		},
		"MemberSourceBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MemberSourceBlob'"
		},
		"REF_Data_P3MemberCSV_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'REF_Data_P3MemberCSV'"
		},
		"STGDBMemberP3_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'STGDBMemberP3'"
		},
		"Sink_StagingDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Sink_StagingDB'"
		},
		"StagingDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'StagingDB'"
		},
		"Staging_P3_DB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Staging_P3_DB'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "abDataFactory1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"MemberSourceBlob_connectionString": {
						"value": "[parameters('MemberSourceBlob_connectionString')]"
					},
					"REF_Data_P3MemberCSV_connectionString": {
						"value": "[parameters('REF_Data_P3MemberCSV_connectionString')]"
					},
					"STGDBMemberP3_connectionString": {
						"value": "[parameters('STGDBMemberP3_connectionString')]"
					},
					"Sink_StagingDB_connectionString": {
						"value": "[parameters('Sink_StagingDB_connectionString')]"
					},
					"StagingDB_connectionString": {
						"value": "[parameters('StagingDB_connectionString')]"
					},
					"Staging_P3_DB_connectionString": {
						"value": "[parameters('Staging_P3_DB_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "abDataFactory1_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'abDataFactory1_ArmTemplate_0')]"
			]
		},
		{
			"name": "abDataFactory1_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'abDataFactory1_ArmTemplate_1')]"
			]
		}
	]
}